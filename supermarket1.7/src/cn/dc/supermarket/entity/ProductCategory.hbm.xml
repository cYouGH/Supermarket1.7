<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.dc.supermarket.entity">

	<class name="ProductCategory" table="_productCategory">
	
		<id name="cid" type="integer" column="cid" >
			<generator class="increment"></generator>
		</id>
		<property name="categoryName" type="string" column="categoryName"></property>
		

	</class>

</hibernate-mapping>